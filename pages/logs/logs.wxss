.log-list {
  display: flex;
  flex-direction: column;
}
.list {
  position: fixed;
  width: 96%;
  top: 500rpx;
}
.log-item {
  display: block;
  margin: 10rpx;
}
#map {
  position: fixed;
  width: 100%;
  height: 500rpx;
  top: 0;
  border-bottom: 2px solid black;
}
.map_container{
  position: absolute;
  top: 0;
  height: 500rpx;
  left: 0;
  right: 0;
}
.map{
  width: 100%;
  height: 100%;
}
.map_text{
  position: absolute;
  left: 0;
  right: 0;
  top: 0px;
  height: 80px;
  background: rgba(255, 255, 255, .7);
  padding: 0 15px;
}
text{
  margin: 5px 0;
  display: block;
  font-size:12px;
}
.h1{
  margin: 15px 0;
  font-size:15px;
}
